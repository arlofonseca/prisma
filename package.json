{
  "name": "prisma",
  "description": "JavaScript/TypeScript tool containing tests to use Prisma ORM within FiveM",
  "author": "arlofonseca",
  "license": "MIT",
  "type": "module",
  "version": "1.0.2",
  "scripts": {
    "pull": "npx prisma db pull",
    "generate": "npx prisma generate",
    "generate:sql": "npx prisma generate --sql",
    "studio": "npx prisma studio",
    "build": "node build.js",
    "watch": "node build.js --watch",
    "ci": "pnpm run lint && pnpm run build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "format": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlofonseca/prisma.git"
  },
  "bugs": "https://github.com/arlofonseca/prisma/issues",
  "dependencies": {
    "@nativewrappers/fivem": "^0.0.42",
    "@overextended/ox_core": "^1.0.1",
    "@overextended/ox_lib": "latest",
    "@prisma/client": "^5.21.1"
  },
  "devDependencies": {
    "@citizenfx/server": "latest",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "esbuild": "^0.24.0",
    "esbuild-plugin-fileloc": "^0.0.6",
    "eslint": "^8.56.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16.9.1"
  }
}
