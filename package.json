{
  "name": "prisma",
  "description": "JavaScript/TypeScript tool containing examples of how to use Prisma ORM for FiveM",
  "author": "arlofonseca",
  "license": "MIT",
  "type": "module",
  "version": "1.0.3",
  "scripts": {
    "pull": "npx prisma db pull",
    "generate": "npx prisma generate",
    "generate:sql": "npx prisma generate --sql",
    "studio": "npx prisma studio",
    "build": "node build.js",
    "watch": "node build.js --watch",
    "ci": "pnpm run check && pnpm run build",
    "check": "pnpm biome format",
    "biome-fix": "pnpm biome format --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlofonseca/prisma.git"
  },
  "bugs": "https://github.com/arlofonseca/prisma/issues",
  "dependencies": {
    "@nativewrappers/fivem": "^0.0.42",
    "@overextended/ox_core": "latest",
    "@overextended/ox_lib": "latest",
    "@prisma/client": "^6.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@citizenfx/server": "latest",
    "@types/node": "^22.8.4",
    "esbuild": "^0.25.0",
    "esbuild-plugin-fileloc": "^0.0.6",
    "prisma": "^6.3.0",
    "typescript": "^5.6.3"
  }
}
